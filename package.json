{
  "name": "vectorinspector",
  "version": "1.0.0",
  "description": "Mapbox GL JS plugin for inspecting vector data",
  "main": "index.js",
  "scripts": {
    "test": "eslint lib && eslint index.js",
    "watch": "mkdir -p dist && watchify index.js --debug -v --standalone MapboxInspector -o dist/vectorinspector.js",
    "build": "mkdir -p dist && NODE_ENV=production browserify index.js --standalone MapboxInspector | uglifyjs - --screw-ie8 --compress --mangle -o dist/vectorinspector.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasmartinelli/vectorinspector.git"
  },
  "keywords": [
    "mapbox",
    "gis",
    "vector",
    "leaflet",
    "gl"
  ],
  "author": "Lukas Martinelli",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/lukasmartinelli/vectorinspector/issues"
  },
  "homepage": "https://github.com/lukasmartinelli/vectorinspector#readme",
  "dependencies": {
    "lodash.isequal": "^4.4.0",
    "randomcolor": "^0.4.4"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "eslint": "^2.8.0",
    "eslint-config-mourner": "^2.0.1",
    "uglify-js": "^2.4.24"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true
    },
    "extends": "mourner",
    "rules": {
      "indent": [2, 2]
    }
  }
}
